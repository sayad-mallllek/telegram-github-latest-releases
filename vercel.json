{
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@4.3.0"
    },
    "api/check.py": {
      "runtime": "@vercel/python@4.3.0"
    }
  },
  "routes": [
    {
      "src": "/api/check",
      "dest": "/api/check.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
